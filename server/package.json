{
	"name": "tidy-server",
	"version": "1.0.0",
	"description": "Server-side code for tidy.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"start": "ts-webpack-builder --isDevelopmentForDebug",
		"test": "npm run test-build && npm run test-again",
		"test-build": "tsc --p test/tsconfig.json",
		"test-again": "node test/test.js",
		"debug": "node --inspect-brk=9229 test/test.js"
	},
	"author": "Andrew Messier",
	"license": "MIT",
	"dependencies": {
		"luxon": "^1.23.0",
		"node-fetch": "^2.6.0",
		"seedrandom": "^3.0.5"
	},
	"devDependencies": {
		"@types/luxon": "^1.22.0",
		"@types/node-fetch": "^2.5.6",
		"@types/node": "^13.11.1",
		"@types/seedrandom": "^2.4.28",
		"typescript": "^3.8.3"
	}
}
