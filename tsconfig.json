{
	"compilerOptions": {
		"allowJs": false,
		"alwaysStrict": true,
		"checkJs": false,
		"jsx": "react",
		"module": "esnext",
		"noImplicitAny": false,
		"noImplicitReturns": false,
		"noImplicitThis": false,
		"outDir": "./build",
		"target": "es2017",
		"moduleResolution": "node"
	},
	"exclude": [
		"node_modules"
	],
	"include": [
		"src/**/*"
	],
	"compileOnSave": false
}